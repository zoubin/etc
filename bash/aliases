# alias grep='grep --color=auto'
function ls() {
  command ls -GF "$@"
}
function ll() {
  ls -lh "$@"
}
function cnpm() {
  npm --registry=https://registry.npm.taobao.org --cache=$HOME/.npm/.cache/cnpm --disturl=https://npm.taobao.org/dist --userconfig=$HOME/.cnpmrc "$@"
}
